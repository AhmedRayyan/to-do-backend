    "compilerOptions": {
        "module": "node16",
        "moduleResolution": "Node16",
        "target": "ES2020",
        "jsx": "react",
        "allowImportingTsExtensions": true,
        "checkJs": false,
        "strictNullChecks": true,
        "strictFunctionTypes": true
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}